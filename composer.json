{
    "name": "codezero/dotenv-updater",
    "description": "Update key/value pairs in a .env file.",
    "keywords": [
        "php",
        "dotenv",
        "env",
        "editor",
        "updater"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Ivan Vermeyen",
            "email": "ivan@codezero.be"
        }
    ],
    "require": {
        "php": "^7.1|^8.0"
    },
    "require-dev": {
        "illuminate/support": "^5.6|^6.0|^7.0|^8.0|^9.0",
        "phpunit/phpunit": "^7.0|^8.0|^9.0"
    },
    "scripts": {
        "test": "phpunit"
    },
    "autoload": {
        "psr-4": {
            "CodeZero\\DotEnvUpdater\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CodeZero\\DotEnvUpdater\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "CodeZero\\DotEnvUpdater\\Laravel\\DotEnvUpdaterServiceProvider"
            ]
        }
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
